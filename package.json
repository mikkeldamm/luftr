{
  "name": "luftr",
  "version": "0.0.0",
  "description": "Luftr is a website for dog sitters",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikkeldamm/luftr.git"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch:sass": "node-sass --watch --include-path scss ./src/browser.scss ./dist/styles/browser.css",
    "build:sass": "node-sass --include-path scss ./src/browser.scss ./dist/styles/browser.css",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config webpack.dev.config.js --progress --profile --colors --display-error-details --display-cached && npm run build:sass",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached --bail && npm run build:sass",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached  --content-base dist/",
    "server:prod:old": "http-server dist --cors",
    "start": "node server.js",
    "bower-install": "bower install",
    "typings-install": "typings install",
    "postinstall": "npm run bower-install && npm run typings-install"
  },
  "license": "MIT",
  "contributors": [
    "Mikkel Damm <mikkeldamm@hotmail.com>"
  ],
  "dependencies": {
    "angular2-jwt": "^0.1.7",
    "express": "^4.13.4",
    "http-server": "^0.8.5",
    "jwt-decode": "^1.5.1"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.8",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.6.0",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15",
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "es6-promise-loader": "^1.0.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.1.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "parse5": "^1.3.2",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4",
    "protractor": "^3.1.1",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.5.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.5.5",
    "tsconfig-lint": "^0.6.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.3.12",
    "typescript": "1.7.5",
    "typings": "^0.6.8",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "bower": "^1.7.7",
    "sass-loader": "^3.1.2",
    "node-sass": "^3.4.2",
    "extract-text-webpack-plugin": "^1.0.1"
  }
}
